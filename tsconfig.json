{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types", "jest"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "rootDir": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
